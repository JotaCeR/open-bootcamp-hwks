

public class Main {
    
    public static int sumar(int a, int b, int c) {
        return a + b + c;
    };

    suma(10, 20, 80);

    Coche miCoche = new Coche();

    miCoche.AñadirPuerta();

    System.out.println(miNave.puertas);
};

class Coche {
    public int puertas = 0;

    public void AñadirPuerta() {
        this.puertas++;
    };
};